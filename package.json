{
  "name": "@zipzoong/zipzoong-backend",
  "version": "0.0.0",
  "description": "backend for zipzoong service",
  "main": "build/main",
  "scripts": {
    "check": "ts-patch check",
    "prepare": "ts-patch install",
    "build": "rimraf build && tsc -p ./tsconfig.build.json",
    "build:api": "rimraf packages/api/lib && npm run nestia && tsc -p ./tsconfig.api.json",
    "eslint": "eslint .",
    "dev": "npm run build && NODE_ENV=development node build/main",
    "start": "NODE_ENV=production node build/main",
    "test": "rimraf tbuild && tsc -p ./tsconfig.test.json && NODE_ENV=test node tbuild/test",
    "swagger": "NODE_ENV=development nestia swagger",
    "sdk": "rimraf ./src/api/functional && NODE_ENV=development nestia sdk && find ./src/api -type f -exec sed -i '' 's/\\r$//' {} \\;",
    "nestia": "npm run swagger ; npm run sdk",
    "db:sch": "ts-node prisma",
    "db:update": "rimraf db && npm run db:sch && prisma generate",
    "db:sync": "npm run db:update && rimraf prisma/migrations && prisma migrate dev",
    "db:init":"prisma migrate reset",
    "db:deploy": "prisma migrate deploy"
  },
  "author": "industriously",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/zipzoong/zipzoong-backend.git"
  },
  "dependencies": {
    "@devts/nestjs-auth": "^1.3.14",
    "@fxts/core": "^0.14.0",
    "@nestia/core": "^1.2.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typia": "^3.8.4"
  },
  "devDependencies": {
    "@nestia/e2e": "^0.2.1",
    "@nestia/sdk": "^1.2.2",
    "@prisma/client": "^4.13.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nestia": "^4.2.0",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "rimraf": "^5.0.0",
    "schemix": "^1.12.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}